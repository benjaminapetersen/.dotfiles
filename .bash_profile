

# Alias 
# -----------------
# Directory Traversal
alias ..="cd .."
alias ...="cd ../.."
alias ...="cd ../../.."
alias ....="cd ../../../.."
alias .....="cd ../../../../.."
alias ......="cd ../../../../../.."
alias ~="cd ~"
alias --="cd -"




# Path
# -----------------
export PATH=$HOME/bin:/usr/local/bin:$PATH

# Source to execute helpers
# source $HOME/.some-file
# source $HOME/some-other-file
